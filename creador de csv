      "cell_type": "code",
      "source": [
        "import yfinance as yf\n",
        "import pandas as pd\n",
        "import pandas_ta as ta\n",
        "import numpy as np\n",
        "from datetime import datetime, timedelta\n",
        "\n",
        "# 1. Configuración básica\n",
        "TICKER = 'AAPL'\n",
        "dias_atras = 10 * 365\n",
        "fecha_fin = datetime.now()\n",
        "fecha_inicio = fecha_fin - timedelta(days=dias_atras)\n",
        "nombre_archivo = \"AAPL_Completo_10_anios.csv\"\n",
        "\n",
        "print(f\"Descargando datos para {TICKER}...\")\n",
        "\n",
        "# 2. Descarga de datos\n",
        "try:\n",
        "    df = yf.download(TICKER, start=fecha_inicio.date(), end=fecha_fin.date(), progress=True, auto_adjust=False)\n",
        "except Exception as e:\n",
        "    print(f\"Error en la descarga: {e}\")\n",
        "    exit()\n",
        "\n",
        "# Estandarizar nombres de columnas\n",
        "if isinstance(df.columns, pd.MultiIndex):\n",
        "    df.columns = df.columns.to_flat_index().map(lambda x: x[0])\n",
        "df.columns = df.columns.str.lower().str.replace(' ', '_')\n",
        "\n",
        "# 3. EL TRUCO: Reindexar para tener TODOS los días del calendario\n",
        "# Creamos un calendario diario sin saltos\n",
        "calendario = pd.date_range(start=df.index.min(), end=df.index.max(), freq='D')\n",
        "df = df.reindex(calendario)\n",
        "\n",
        "# Rellenar precios de fines de semana (el precio no cambia si no hay mercado)\n",
        "df[['open', 'high', 'low', 'close', 'adj_close']] = df[['open', 'high', 'low', 'close', 'adj_close']].ffill()\n",
        "\n",
        "# El volumen los fines de semana es 0\n",
        "df['volume'] = df['volume'].fillna(0)\n",
        "\n",
        "# Renombrar columnas para el modelo\n",
        "df = df.rename(columns={\n",
        "    'open': 'open_price',\n",
        "    'high': 'day_high',\n",
        "    'low': 'day_low',\n",
        "    'volume': 'current_volume',\n",
        "    'adj_close': 'close_price'\n",
        "})\n",
        "\n",
        "# 4. Cálculo de Indicadores Técnicos\n",
        "# Nota: Ahora los indicadores se calculan sobre 3,650 días seguidos\n",
        "df.ta.sma(length=20, append=True, col_names=('SMA_20',))\n",
        "df.ta.sma(length=200, append=True, col_names=('SMA_200',))\n",
        "df.ta.rsi(length=14, append=True, col_names=('RSI_14',))\n",
        "df.ta.macd(fast=12, slow=26, signal=9, append=True)\n",
        "\n",
        "# Limpiar nombres de MACD\n",
        "df = df.rename(columns={\n",
        "    'MACD_12_26_9': 'MACD',\n",
        "    'MACDh_12_26_9': 'MACD_histogram',\n",
        "    'MACDs_12_26_9': 'MACD_signal'\n",
        "})\n",
        "\n",
        "# 5. Limpieza y Exportación\n",
        "# Quitamos solo las filas iniciales donde no hay suficientes datos para la media de 200\n",
        "df = df.dropna(subset=['SMA_200'])\n",
        "\n",
        "df.index.name = 'timestamp'\n",
        "df.reset_index(inplace=True)\n",
        "df.to_csv(nombre_archivo, index=False)\n",
        "\n",
        "print(f\"¡Listo! Archivo creado con {len(df)} filas.\")\n",
        "print(f\"Nombre del archivo: {nombre_archivo}\")"
      ],
